name: ensemble_learning

conda_env: conda.yaml

entry_points:
  stacking_train:
    parameters:
      dataset: {type: str}
      features: {type: str}
    command: "spark-submit EnsembleStacking.py -t {dataset} -f features"
  stacking_prediction:
    parameters:
      dataset: {type: str}
      features: {type: str}
    command: "spark-submit EnsembleStacking.py -p {dataset} -f features"
  main:
    parameters:
      maxIter: {type: int, default: 5}
    command: "spark-submit main.py {maxIter}"